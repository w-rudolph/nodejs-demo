{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/**/node_modules"
    ],
    "verbose": true,
    "execMap": {
        "js": "node --harmony"
    },
    "watch": [
        "routes/",
        "views/",
        "services/",
        "public/"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js json ejs"
}